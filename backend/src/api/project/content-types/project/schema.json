{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Projects",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "validatingPartner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::partner.partner"
    },
    "monitoringReports": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::monitoring-report.monitoring-report"
    },
    "implementingPartners": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::partner.partner"
    },
    "sdgs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sdg.sdg"
    },
    "targetCompensation": {
      "type": "biginteger",
      "required": true
    },
    "actualCompensation": {
      "type": "biginteger",
      "required": true
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "budget": {
      "type": "biginteger"
    },
    "projectCompensators": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-compensator.project-compensator",
      "mappedBy": "project"
    },
    "location": {
      "type": "string"
    },
    "coordinates": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "projectImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "updates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::update.update",
      "mappedBy": "project"
    },
    "country": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country"
    },
    "type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project-type.project-type"
    },
    "amountFunded": {
      "type": "biginteger"
    },
    "basin": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::basin.basin"
    }
  }
}
