{
  "kind": "collectionType",
  "collectionName": "compensators",
  "info": {
    "singularName": "compensator",
    "pluralName": "compensators",
    "displayName": "Compensators",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "targetPledgeCaps": {
      "type": "biginteger"
    },
    "sdgs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sdg.sdg"
    },
    "projectCompensations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-compensator.project-compensator",
      "mappedBy": "compensator"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "updates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::update.update",
      "mappedBy": "compensator"
    },
    "compensationProgressDescription": {
      "type": "richtext"
    },
    "compensationProgressImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "compensationProgressTitle": {
      "type": "string"
    }
  }
}
